{
    "type": "project",
    "license": "GPL-2.0-or-later",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "require": {
        "php": "^7.4",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/blazy": "^2.1",
        "drupal/colorbox": "^1.6",
        "drupal/core-composer-scaffold": "^9.1",
        "drupal/core-project-message": "^9.1",
        "drupal/core-recommended": "9.2.x-dev",
        "drupal/honeypot": "^2.0",
        "drupal/pathauto": "^1.8",
        "drupal/r4032login": "^2.0",
        "drupal/s3fs": "dev-3.x",
        "drupal/s3fs_cors": "dev-1.x",
        "drupal/vani": "^1.2",
        "npm-asset/blazy": "^1.8",
        "npm-asset/jquery-colorbox": "^1.6",
        "oomphinc/composer-installers-extender": "^2.0"
    },
    "require-dev": {
        "drupal/devel": "^4.0",
        "drupal/migrate_tools": "^5.0",
        "drush/drush": "^10.3"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "dev-site-install": [
            "Composer\\Config::disableProcessTimeout",
            "drush site:install familyhistory --account-pass=password --site-name=\"Family History\"",
            "drush pm:enable devel devel_generate",
            "drush pm:enable family_history_migrate migrate_tools",
            "drush cache:rebuild"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-types": [
            "npm-asset"
        ],
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/colorbox": [
                "npm-asset/jquery-colorbox"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "patches": {
            "drupal/core": {
                "book.module: Use natural sorting": "patches/core_book_use_natural_sorting.patch"
            },
            "drupal/s3fs_cors": {
                "Remove deprecations": "patches/s3fs_cors_remove_deprecations.patch",
                "Custom hostname": "patches/s3fs_cors_custom_hostname.patch"
            },
            "drupal/s3fs": {
                "Remove deprecations": "https://www.drupal.org/files/issues/2020-07-24/use_isValidScheme_method-3161279.patch"
            }
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
